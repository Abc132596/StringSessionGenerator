{
    "name": "String-Session-Generator",
    "description": "A bot for Generating String Session of Telegram Account mostly for Userbots.",
    "repository": "https://github.com/AsmSafone/StringSessionGenerator",
    "keywords": [
        "string",
        "session"
    ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "env": {
        "APP_NAME": {
            "description": "Heroku App Name",
            "value": "",
            "required": false
        },
        "BOT_TOKEN": {
            "description": "Your bot token, as a string.",
            "value": "6630556790:AAFGarOavZLEzV4hhVMZIfYrDv4VJ-LfJL8",
            "required": true
        },
        "API_KEY": {
            "description": "Your Heroku Account API Key from https://dashboard.heroku.com/account ",
            "value": "",
            "required": false
        },
        "API_ID": {
            "description": "Get from https://my.telegram.org",
            "value": "21543397",
            "required": true
        },
        "API_HASH": {
            "description": "Get from https://my.telegram.org",
            "value": "87579ffc7be4d3ababdc07cbbc2e8dc4",
            "required": true
        }
    }
}
